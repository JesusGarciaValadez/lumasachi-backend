{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/vue-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-pcntl": "*",
        "barryvdh/laravel-dompdf": "^3.1",
        "dedoc/scramble": "^0.12.30",
        "inertiajs/inertia-laravel": "^2.0",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.33",
        "laravel/nightwatch": "^1.11",
        "laravel/octane": "^2.12",
        "laravel/sanctum": "^4.2",
        "laravel/telescope": "^5.11",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.0",
        "symfony/http-client": "^7.3",
        "symfony/mailgun-mailer": "^7.3",
        "tightenco/ziggy": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "barryvdh/laravel-ide-helper": "^3.6",
        "brianium/paratest": "^7.8",
        "fakerphp/faker": "^1.23",
        "friendsofphp/php-cs-fixer": "*",
        "laravel/boost": "^1.2",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.44",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "peckphp/peck": "^0.1.3",
        "phpunit/phpunit": "^11.5.3",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "spatie/ray": "^1.42"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --reset --nowrite --write-mixin",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "ide-helper": [
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:models --reset --nowrite --write-mixin",
            "@php artisan ide-helper:meta"
        ],
        "cs-fixer": [
            "@php ./vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php --allow-risky=yes"
        ],
        "larastan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "pint": [
            "./vendor/bin/pint -v"
        ],
        "lint": "pint",
        "refactor": "rector",
        "test:coverage": "@php artisan test --parallel --coverage --min=90",
        "test:lint": "pint --test",
        "test:unit": "@php artisan test --parallel --testsuite=Unit",
        "test:feature": "@php artisan test --parallel --testsuite=Feature",
        "test:types": "phpstan --configuration=./phpstan.neon",
        "test:typos": "peck",
        "test:refactor": "rector --dry-run",
        "test:complete": [
            "@lint",
            "@refactor",
            "@test:types",
            "@test:typos",
            "@test:coverage",
            "@test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
